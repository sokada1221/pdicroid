<?xml version="1.0" encoding="utf-8"?>
<com.reliefoffice.pdic.PSBookmarkItemLayout xmlns:android="http://schemas.android.com/apk/res/android"
android:orientation="vertical" android:layout_width="match_parent"
android:layout_height="match_parent">


<LinearLayout android:id="@+id/psb_list_item_layout"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:orientation="horizontal">
<TextView android:id="@+id/psb_list_item_header"
    android:layout_height="wrap_content"
    android:layout_width="wrap_content"
    android:textSize="18sp"
    android:textColor="#000000"
    />
    <TextView android:id="@+id/psb_list_item_text"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="18sp"
        android:padding="6dp"
        android:textColor="#000000"
        />
    <TextView android:id="@+id/psb_list_item_comment"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="18sp"
        android:textColor="#000000"
        />
</LinearLayout>

</com.reliefoffice.pdic.PSBookmarkItemLayout>

